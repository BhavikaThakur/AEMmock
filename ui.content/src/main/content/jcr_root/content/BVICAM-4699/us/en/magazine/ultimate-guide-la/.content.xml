                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="BVICAM-4699/components/text"
                        text="&lt;blockquote>&amp;nbsp;“There is no better place to shred then Los Angeles.”.&lt;/blockquote>&#xd;&#xa;&lt;p>-Jacob Wester, Pro Skater&lt;/p>&#xd;&#xa;"
